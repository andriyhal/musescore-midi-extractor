generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-arm64-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Score {
  id               Int      @id @default(autoincrement())
  title            String?
  url              String   @unique
  publisher        String?
  composer         String?
  date_created     Int?
  date_updated     Int?
  pages            Int?
  duration         String?
  info             String?
  measures         Int?
  keysig           String?
  difficultyLevel  Int?
  genres           String[]
  instrumentations String[]
  instruments      String[]
  categoryPages    String[]
  scoresJson       Json?
  count_views      Int?
  count_favorites  Int?
  count_comments   Int?
  rating           Float?
  rating_count     Int?
}

enum ProxyStatus {
  available
  busy
  forbidden
}

model Proxy {
  id       Int         @id @default(autoincrement())
  ip       String
  port     Int
  login    String
  password String
  status   ProxyStatus

  @@unique([ip, port])
}
